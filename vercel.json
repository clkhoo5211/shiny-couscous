{
  "version": 2,
  "buildCommand": "cd backend && echo 'No build needed for Python'",
  "outputDirectory": ".",
  "framework": null,
  "installCommand": "cd backend && pip install -r requirements.txt",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}

